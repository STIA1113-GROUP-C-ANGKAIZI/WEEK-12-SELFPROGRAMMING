package selfProgramming2;
import java.util.Scanner;
public class TheGreatestNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner scan  = new Scanner (System.in);
		int a, b, c;
		
		System.out.println("Please insert the first number: ");
		a = scan.nextInt();
		System.out.println("Please insert the second number: ");
		b = scan.nextInt();
		System.out.println("Please insert the third number: ");
		c = scan.nextInt();
		
		if (a > b)
			if (a > c)
				System.out.println("The greatest is " + a);
		
		if (b > a)
			if (b > c)
				System.out.println("The greatest is " + b);
		
		if (c > a)
			if (c > b)
				System.out.println("The greatest is " + c);
	}

}

Output:
Please insert the first number: 
89
Please insert the second number: 
50
Please insert the third number: 
99
The greatest is 99
